<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="AirBnb2" transaction-type="RESOURCE_LOCAL">
		<class>model.Image</class>
		<class>model.ImagePK</class>
		<class>model.Listing</class>
		<class>model.ListingPK</class>
		<class>model.Message</class>
		<class>model.MessagePK</class>
		<class>model.Reservation</class>
		<class>model.ReservationPK</class>
		<class>model.Review</class>
		<class>model.ReviewPK</class>
		<class>model.SearchHistory</class>
		<class>model.SearchHistoryPK</class>
		<class>model.User</class>
		<class>model.VisitedHistory</class>
		<class>model.VisitedHistoryPK</class>
		<class>model.Recommendation</class>
		<class>model.RecommendationPK</class>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/airbnbdb14"/>
			<property name="javax.persistence.jdbc.user" value="root"/>
			<property name="javax.persistence.jdbc.password" value="1234"/>
			<property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
		</properties>
	</persistence-unit>
</persistence>
